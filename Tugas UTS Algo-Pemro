public class MyClass{
    public static void main(String[] args){

        String nama = "Nama Saya :";
        String firstName = "Ryan";
        String lastName = "Mulyadi";
        String full = nama + firstName + lastName;

        int myYearBorn = 2003;
        String lahir ="Saya Lahir di Tahun :";
        String tahunLahir = myYearBorn + lahir;
        
        System.out.println("Assalamu'alaikum");
        System.out.println(full); 
        System.out.println(tahunLahir);
    }

}
